DEPTH = 256;
WIDTH = 8;
ADDRESS_RADIX = HEX;
DATA_RADIX = BIN;
CONTENT
BEGIN
0 : 00000011; -- ADD A + imm
1 : 00000001; -- imm = 1
2 : 00000111; -- ADD B + imm
3 : 00000010; -- imm = 2
4 : 00001010; -- ADD R + R
5 : 00010011; -- SUB A - imm
6 : 00000001; -- imm = 1
7 : 00010111; -- SUB B - imm
8 : 00000010; -- imm = 2
9 : 00011010; -- SUB R - R
A : 00100011; -- AND A and imm
B : 00000001; -- imm = 1
C : 00100111; -- AND B and imm
D : 00000010; -- imm = 2
E : 00101010; -- AND R and R
END;
